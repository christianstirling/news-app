
<div class="upload-container">

    <div class="upload-top admin-top">

        <div class="pagebutton-container">
            <a href="/admin/dashboard"><button class="">BACK TO DASHBOARD</button></a>

            <a href="/admin/photo">
                <button>BACK TO VIEW PHOTOS</button>
            </a>
        </div>

        <div class="pagetitle-container"><h1 class="dashboard-title">Article Logo Photos</h1></div>

    </div>

    <div class="admin-middle">

        <form action="/admin/upload" method="POST" enctype="multipart/form-data" class="upload-form">

            <label for="imageFile">Choose an image:</label><br>
            <input type="file" name="imageFile" id="imageFile" accept="image/*" required><br><br>
    
            <label for="filename">Enter a name for the file (do not include an extension):</label><br>
            <input type="text" name="filename" id="filename" required><br><br>
    
            <button type="submit">UPLOAD IMAGE</button>
    
        </form>

    </div>

    









</div>



